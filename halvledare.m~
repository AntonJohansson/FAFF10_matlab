th_ge = get_normalized_spe_data('data/Th_Ge.Spe');
th    = get_normalized_spe_data('data/Th_spektrum.Spe');
filter_20x = get_bg_filter('data/bakgrund_20x.Spe');

new_data = zeros(2*length(th), 1);
for i=1:length(th)
    new_data(2*i) = 2*th(i);
end
new_data = new_data(1:length(th));

figure;
hold on;
plot(1:length(th_ge), th_ge);
%plot(1:length(th), 2*th);
plot(1:length(new_data), new_data(20:);
xlim([0 2048]);